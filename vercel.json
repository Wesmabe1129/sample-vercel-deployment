{
    "version": 2,
    "builds": [
        {
            "src": "dist/server/server-entry.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/client/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/.*",
            "dest": "/dist/server/server-entry.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/server/server-entry.js"
        }
    ]
}